export MAIN_CLASS_NAME=<CLASS_NAME>
export project_id=<PROJECT_ID>
export REGION=<REGION>
export TEMP_LOCATION=<TEMP_LOCATION>
export PIPELINE_FOLDER=<PIPELINE_TEMPLATE_LOCATION>
export SUBNETWORK_NAME=<SUBNETWORK_NAME>
export JOB_NAME=<DATAFLOW_JOB_NAME>
export filePath=<JSON_FILE_PATH_FROM_GCS>
export database_name=<FIRESTORE_DATABAE_NAME>
export collection_name=<FIRESTORE_COLLECTION_NAME>
export batch_Size=<BATCH_SIZE>


 mvn compile exec:java -Dexec.mainClass=${MAIN_CLASS_NAME} -Dexec.cleanupDaemonThreads=false "-Dexec.args= --project=$project_id --jobName=$JOB_NAME --runner=DataflowRunner --region=$REGION --stagingLocation=${PIPELINE_FOLDER}/staging --gcpTempLocation=$TEMP_LOCATION --subnetwork=https://www.googleapis.com/compute/v1/projects/$GOOGLE_CLOUD_PROJECT/regions/$REGION/subnetworks/$SUBNETWORK_NAME --gcsPath=$filePath --database_name=$database_name --collection_name=$collection_name --batch_Size=$batch_Size --projectId=$project_id"
